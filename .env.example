# XCreator Pro + Eliza Integration - Environment Configuration Template
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:password@postgres:5432/xcreator_eliza

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379

# Application Configuration
NODE_ENV=development
PORT=3000
JWT_SECRET=change-this-to-a-secure-random-string
LOG_LEVEL=info

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:4001

# Eliza Service Configuration
ELIZA_BASE_URL=http://eliza:4001

# n8n Integration
N8N_API_URL=https://your-n8n-instance.com/api/v1
N8N_API_TOKEN=your-n8n-api-token-here

# AI Services (Optional - for Phase 2/3)
OPENAI_API_KEY=your-openai-api-key
ELEVENLABS_API_KEY=your-elevenlabs-api-key
OPENROUTER_API_KEY=your-openrouter-api-key
RUNPOD_API_KEY=your-runpod-api-key
HUGGINGFACE_API_KEY=your-huggingface-api-key

# Supabase Configuration (Optional)
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key

# Monetization (Optional)
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret
AFFILIATE_AIPORN_KEY=your-affiliate-key
ADULT_SHOP_API_KEY=your-shop-api-key
REVENUE_SHARE_PERCENTAGE=15

# External Services
APP_URL=http://localhost:3000
